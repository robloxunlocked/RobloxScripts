local message = {
	Title = "PessimScripts";
	Text = ".gg/5kEV23AqJD";
	Icon = "rbxassetid://375691701";
	Duration = 5;
}

game:GetService("StarterGui"):SetCore("SendNotification", message)

game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2422.09033203125, 4097.08935546875, 240.86151123046875)

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

local newPart = Instance.new("Part")
newPart.Size = Vector3.new(5, 1, 5)
newPart.Position = humanoidRootPart.Position - Vector3.new(0, humanoidRootPart.Size.Y/2 + newPart.Size.Y/2, 0)
newPart.Transparency = 1
newPart.Anchored = true
newPart.Parent = workspace

humanoid.PlatformStand = true
humanoid.Sit = true

